<body class="fonts container-lg">
    <section class="px-4" style="margin-top: 10vw; margin-bottom: 10vw;">
        <h1>Add Labs</h1>
        <form action='labs' method='POST'>
            <label for="labname" class="form-label">Lab Name</label>
            <input type="text" name="labname" id="labname" class="form-control mb-3">
